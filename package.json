{
    "name": "testsense-dashboard-api",
    "version": "0.1.0",
    "description": "Dashboard API for TestsenseAI",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "nitric start",
        "build": "tsc",
        "deploy": "nitric up",
        "test": "NODE_ENV=test jest --config=jest.config.ts",
        "test:watch": "NODE_ENV=test jest --config=jest.config.ts --watch",
        "test:coverage": "NODE_ENV=test jest --config=jest.config.ts --coverage",
        "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0 --cache --cache-location ./node_modules/.cache/eslint",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist coverage node_modules/.cache"
    },
    "dependencies": {
        "@nitric/sdk": "^1.3.3",
        "@types/axios": "^0.14.4",
        "@types/ioredis": "^5.0.0",
        "axios": "^1.7.9",
        "date-fns": "^4.1.0",
        "ioredis": "^5.5.0",
        "jwt-decode": "^4.0.0",
        "openai": "^4.28.0",
        "superjson": "^2.2.1",
        "uuid": "^11.0.5",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.0",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "globals": "^15.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3",
        "vitest": "^3.0.4"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
